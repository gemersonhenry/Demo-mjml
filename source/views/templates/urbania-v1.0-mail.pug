				mj-column
					mj-text(mj-class="align-r color-green-urb font-h1",
						padding-right=paddingCustomSM)
						| BZGemerson
			mj-section(padding-top=paddingNull, padding-bottom=paddingNull)
				mj-column(mj-class="bg-green-urb")
					mj-text(mj-class="font-lg color-white align-c",
						padding-top=paddingCustomLG, padding-bottom=paddingCustomLG)
