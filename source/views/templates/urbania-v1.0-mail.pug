- var title = "Mira los inmuebles que hemos encontrado para ti"
mjml
	mj-head
		mj-attributes
			+head-attributes("urbania")
		mj-style(inline="inline").
			.padding-trl{
				padding-top: 20px;
				padding-right: 20px;
				padding-left: 20px;
			}
			.padding-right{
				padding-right: 20px;
			}
			.padding-left{
				padding-left: 20px;
			}
			.box-sizing{
				box-sizing: border-box;
			}
			.padding-sm{
				padding: 5px;
			}
			.padding-md{
				padding: 15px;
			}
			.padding-lg{
				padding: 25px;
			}
	mj-body
		mj-container(mj-class="bg-gray")
			//- header
			mj-section(padding-top=paddingNull, padding-bottom=paddingNull, mj-class="bg-white")
				mj-column
					mj-text(mj-class="align-l color-green-urb font-h1",
						padding-left=paddingCustomSM)
